@model AdminCampana_2020.ViewModels.UsuarioRolVM
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}
<noscript>
    <meta http-equiv="refresh" content="0;URL=@Url.Action("NoScript", "Error")" />
</noscript>
<div class="bs-container" style="min-height:960px;">

    <section class="content-header">
        <h1>
            Movilizador
            <small>(Consultar)</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"> <i class="fa fa-dashboard"></i> Inicio </a></li>
            <li> <a href="#">Movilizadores</a></li>
            <li class="active">Consultar Datos Personales </li>
        </ol>
    </section>
    <br />
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="box box-success">
                <div class="box-header with-border">
                    <h5 class="box-title">
                        <label class="text-success emailtitleletter">
                            <i class="text-success fa fa-graduation-cap"></i>
                            @User.Identity.Name
                            (@ViewBag.Role)
                        </label>&nbsp;

                    </h5>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-8">
                            <div class="box box-success">
                                <div class="box-header with-border">
                                    <h3 class="box-title"><span class="fas fa-chart-line"></span> Control Estadistico</h3>
                                    <div class="box-tools pull-right">
                                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                        <button type="button" class="btn btn-box-tool" data-widget="remove">
                                            <i class="fa fa-times"></i>
                                        </button>
                                    </div>
                                </div>
                                <div class="box-body no-padding">
                                    <div class="row">
                                        <div class="col-md-9 col-sm-8">
                                            <div class="pad">
                                                <div class="form-group">
                                                    <label>Area:&nbsp;&nbsp;</label>
                                                    @Html.DropDownList("IdRol", null, new { @class = "form-control input-sm", placeholder = "", required = "", id = "idRol" })
                                                    @Html.ValidationMessageFor(model => model.IdRol, "", new { @class = "text-danger" })
                                                </div>
                                                <div class="form-group">
                                                    <label>Coordinador&nbsp;&nbsp;</label>
                                                    <select class="form-control" id="idNombres"></select>
                                                </div>
                                                <div class="form-group">
                                                    <label><span class="fas fa-user-tie"></span> Nombre:&nbsp;&nbsp;</label>
                                                    <input type="text" class="form-control" id="Nombres" disabled="disabled" placeholder="Nombre" />
                                                </div>
                                                <div class="form-group">
                                                    <label><span class="fas fa-user-check"></span>Apellidos:&nbsp;&nbsp;</label>
                                                    <input type="text" class="form-control" id="Apellidos" disabled="disabled" placeholder="Apellidos" />
                                                </div>
                                                <div class="form-group">
                                                    <label><span class="fas fa-at"></span>Email:&nbsp;&nbsp;</label>
                                                    <input type="email" class="form-control" id="email" disabled="disabled" placeholder="Email" />
                                                </div>
                                                <div class="form-group">
                                                    <input type="button" value="Consultar" class="btn btn-sm btn-primary" id="btnConsultar" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3 col-sm-4">
                                            <div class="pad box-pane-right bg-green" style="min-height: 280px">
                                                <div class="description-block margin-bottom">
                                                    <div class="sparkbar pad" data-color="#fff">
                                                        <i class="fas fa-chart-pie"></i>
                                                    </div>
                                                    <h5 class="description-header">@ViewBag.totalMultinivel</h5>
                                                    <span class="description-text">Multinivel</span>
                                                </div>
                                                <div class="description-block margin-bottom">
                                                    <div class="sparkbar pad" data-color="#fff">
                                                        <i class="fas fa-chart-area"></i>
                                                    </div>
                                                    <h5 class="description-header">@ViewBag.totalPlanillaGanadora</h5>
                                                    <span class="description-text">Planilla Ganadora</span>
                                                </div>
                                                <div class="description-block margin-bottom">
                                                    <div class="sparkbar pad" data-color="#fff">
                                                        <i class="far fa-chart-bar"></i>
                                                    </div>
                                                    <h5 class="description-header">@ViewBag.totalCampania</h5>
                                                    <span class="description-text">Campaña</span>
                                                </div>
                                                <div class="description-block margin-bottom">
                                                    <div class="sparkbar pad" data-color="#fff">
                                                        <i class="fas fa-users"></i>
                                                    </div>
                                                    <h5 class="description-header">@ViewBag.totalEnCampania</h5>
                                                    <span class="description-text">En Campaña</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="info-box bg-yellow animated bounceInRight">
                                <span class="info-box-icon">
                                    <i class="fas fa-users"></i>
                                </span>
                                <div class="info-box-content">
                                    <span class="info-box-text">Estructura Complementaria</span>
                                    <span class="info-box-number">5,200</span>
                                    <div class="progress">
                                        <div class="progress-bar" style="width:50%"></div>
                                    </div>
                                    <span class="progress-description">50% Increase in 30 Days</span>
                                </div>
                            </div>
                            <div class="info-box bg-green animated bounceInRight">
                                <span class="info-box-icon">
                                    <i class="fas fa-user-check"></i>
                                </span>
                                <div class="info-box-content">
                                    <span class="info-box-text">Planilla Gnadora</span>
                                    <span class="info-box-number">92,050</span>
                                    <div class="progress">
                                        <div class="progress-bar" style="width: 20%"></div>

                                    </div>
                                    <span class="progress-description">20% Incremento in 30 Days</span>
                                </div>
                            </div>
                            <div class="info-box bg-red animated bounceInRight">
                                <span class="info-box-icon">
                                    <i class="far fa-address-card"></i>
                                </span>
                                <div class="info-box-content">
                                    <span class="info-box-text">Campaña</span>
                                    <span class="info-box-number">114,381</span>
                                    <div class="progress">
                                        <div class="progress-bar" style="width: 70%"></div>
                                    </div>
                                    <span class="progress-description">70% Increase in 30 Days</span>
                                </div>
                            </div>
                            <div class="info-box bg-aqua animated bounceInRight">
                                <span class="info-box-icon">
                                    <i class="fas fa-laptop-house"></i>
                                </span>
                                <div class="info-box-content">
                                    <span class="info-box-text">En Campaña</span>
                                    <span class="info-box-number">164,921</span>
                                    <div class="progress">
                                        <div class="progress-bar" style="width: 40%"></div>
                                    </div>
                                    <span class="progress-description">40% Increase in 30 Days</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-9">
                            <div class="box box-success">
                                <div class="box-header with-border">
                                    <h3 class="box-title"><span class="fas fa-chart-line"></span>Registros de Movilizados</h3>
                                    <div class="box-tools pull-right">
                                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                        <button type="button" class="btn btn-box-tool" data-widget="remove">
                                            <i class="fa fa-times"></i>
                                        </button>
                                    </div>
                                </div>
                                <div class="box-body no-padding">
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12">
                                            <table id="MyDataTable" class="display">
                                                <thead>
                                                    <tr>
                                                        <th>
                                                            Nombre del Movilizador
                                                        </th>
                                                        <th>
                                                            Apellidos
                                                        </th>
                                                        <th>
                                                            Nombre del Movilizado
                                                        </th>
                                                        <th>
                                                            Apellido Paterno
                                                        </th>
                                                        <th>
                                                            Apellido Materno
                                                        </th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="box box-primary">
                                <div class="box-body box-profile">
                                    <img class="profile-user-img img-responsive img-circle" src="~/Image/tavo.jpg" alt="User profile picture">
                                    <h3 class="profile-username text-center">Octavio Magaña Soto</h3>
                                    <p class="text-muted text-center">Candidato Presidencial, Campaña 2020</p>
                                    <ul class="list-group list-group-unbordered">
                                        <li class="list-group-item">
                                            <b>Movilizados</b> <a class="pull-right">@ViewBag.TotalMovilizados</a>
                                        </li>
                                        <li class="list-group-item">
                                            <b>Coordinadores</b> <a class="pull-right">@ViewBag.TotalCoordinadores</a>
                                        </li>
                                        <li class="list-group-item">
                                            <b>Estructura Política</b> <a class="pull-right">@ViewBag.MetaTotal</a>
                                        </li>
                                    </ul>
                                    <a href="#" class="btn btn-primary btn-block"><b>Enviar Resumen </b></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<link href="~/Content/card-presentation.css" rel="stylesheet" />
<link href="~/Content/animate.min.css" rel="stylesheet" />
<link href="~/Content/toastr.min.css" rel="stylesheet" />
<link href="~/Content/DataTableCss/dataTables.bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/DataTableJs/jquery.js?v=1"></script>
<script src="~/Scripts/manager/consultar-coordinador.js?v=1"></script>
<script src="~/Scripts/toastr.min.js"></script>



